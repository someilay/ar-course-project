{
  "dockerComposeFile": "${localWorkspaceFolder}/docker-compose.yml",
  "service": "ros-humble",
  "workspaceFolder": "/home/ros/course-project",
  "shutdownAction": "stopCompose",
  "initializeCommand": "xhost +local:",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.vscode-pylance",
        "ms-python.python",
        "ms-python.black-formatter",
        "ms-vscode.cpptools",
        "ms-vscode.cpptools-extension-pack",
        "ms-vscode.cpptools-themes",
        "ms-vscode.cmake-tools",
        "redhat.vscode-xml",
        "kylinideteam.cmake-intellisence"
      ]
    }
  },
  "containerEnv": {
    "HTTP_PROXY": "${localEnv:HTTP_PROXY}",
    "HTTPS_PROXY": "${localEnv:HTTPS_PROXY}",
    "NO_PROXY": "${localEnv:NO_PROXY}",
    "FTP_PROXY": "${localEnv:FTP_PROXY}",
    "http_proxy": "${localEnv:http_proxy}",
    "https_proxy": "${localEnv:https_proxy}",
    "no_proxy": "${localEnv:no_proxy}",
    "ftp_proxy": "${localEnv:ftp_proxy}"
  }
  // "mounts": [
  //   "source=${localEnv:HOME}/.config/Code,target=/home/ros/.config/Code,type=bind"
  // ]
}